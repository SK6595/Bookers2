<%= render 'books/list', book: @book, user: @user %>

<table class="table">
<thead>
 <tr>
    <th>
     image
    </th>
    <th>
      name
    </th>
    <th>
   
    </th>
  </tr>
</thead>
<tbody>
 <% @users.each do |user| %>
  <tr>
    <td>
      <%= link_to user_path(user) do %>
      <%= image_tag user.get_profile_image %>
      <% end %>
    </td>
    <td>
      <%= user.name %>
    </td>
    <td>
      <%= link_to "Show", user_path(user) %>
    </td>
  </tr>
  <% end %>
</tbody>
</table>